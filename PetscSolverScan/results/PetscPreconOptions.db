@PARAMETER -pc_telescope_subcomm_type INTERLACED   #  <INTERLACED> Subcomm type (interlaced or contiguous) (choose one of) GENERAL CONTIGUOUS INTERLACED (PCTelescopeSetSubcommType) 
@PARAMETER -pc_telescope_reduction_factor 1   #  <1>: Factor to reduce comm size by (PCTelescopeSetReductionFactor) 
@PARAMETER -pc_telescope_ignore_dm FALSE   # : <FALSE> Ignore any DM attached to the PC (PCTelescopeSetIgnoreDM) 
@PARAMETER -pc_telescope_ignore_kspcomputeoperators FALSE   # : <FALSE> Ignore method used to compute A (PCTelescopeSetIgnoreKSPComputeOperators) 

