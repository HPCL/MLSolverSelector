# List of options for each preconditioner 

@PRECON asm 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_asm_dm_subdomains FALSE   # : <FALSE> Use DMCreateDomainDecomposition() to define subdomains (PCASMSetDMSubdomains) 
@PARAMETER -pc_asm_blocks -1   #  <-1>: Number of subdomains (PCASMSetTotalSubdomains) 
@PARAMETER -pc_asm_overlap 1   #  <1>: Number of grid points overlap (PCASMSetOverlap) 
@PARAMETER -pc_asm_type RESTRICT   #  <RESTRICT> Type of restriction/extension (choose one of) NONE RESTRICT INTERPOLATE BASIC (PCASMSetType) 
@PARAMETER -pc_asm_local_type ADDITIVE   #  <ADDITIVE> Type of local solver composition (choose one of) ADDITIVE MULTIPLICATIVE SYMMETRIC_MULTIPLICATIVE SPECIAL SCHUR (PCASMSetLocalType) 
@PARAMETER -pc_asm_sub_mat_type (null)   #  <(null)>: Subsolve Matrix Type (one of) schurcomplement (PCASMSetSubMatType) 

@PRECON bddc 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_bddc_check_level 0   #  <0>: Verbose output for PCBDDC (intended for debug) (none) 
@PARAMETER -pc_bddc_dirichlet_approximate FALSE   # : <FALSE> Inform PCBDDC that we are using approximate Dirichlet solvers (none) 
@PARAMETER -pc_bddc_dirichlet_approximate_scale FALSE   # : <FALSE> Inform PCBDDC that we need to scale the Dirichlet solve (none) 
@PARAMETER -pc_bddc_neumann_approximate FALSE   # : <FALSE> Inform PCBDDC that we are using approximate Neumann solvers (none) 
@PARAMETER -pc_bddc_neumann_approximate_scale FALSE   # : <FALSE> Inform PCBDDC that we need to scale the Neumann solve (none) 
@PARAMETER -pc_bddc_use_local_mat_graph TRUE   # : <TRUE> Use or not adjacency graph of local mat for interface analysis (none) 
@PARAMETER -pc_bddc_graph_maxcount 2147483647   #  <2147483647>: Maximum number of shared subdomains for a connected component (none) 
@PARAMETER -pc_bddc_corner_selection FALSE   # : <FALSE> Activates face-based corner selection (none) 
@PARAMETER -pc_bddc_use_vertices TRUE   # : <TRUE> Use or not corner dofs in coarse space (none) 
@PARAMETER -pc_bddc_use_edges TRUE   # : <TRUE> Use or not edge constraints in coarse space (none) 
@PARAMETER -pc_bddc_use_faces FALSE   # : <FALSE> Use or not face constraints in coarse space (none) 
@PARAMETER -pc_bddc_vertex_size 1   #  <1>: Connected components smaller or equal to vertex size will be considered as primal vertices (none) 
@PARAMETER -pc_bddc_use_true_nnsp FALSE   # : <FALSE> Use near null space attached to the matrix without modifications (none) 
@PARAMETER -pc_bddc_use_qr_single FALSE   # : <FALSE> Use QR factorization for single constraints on cc (QR is always used when multiple constraints are present) (none) 
@PARAMETER -pc_bddc_use_change_of_basis FALSE   # : <FALSE> Use or not internal change of basis on local edge nodes (none) 
@PARAMETER -pc_bddc_use_change_on_faces FALSE   # : <FALSE> Use or not internal change of basis on local face nodes (none) 
@PARAMETER -pc_bddc_switch_static FALSE   # : <FALSE> Switch on static condensation ops around the interface preconditioner (none) 
@PARAMETER -pc_bddc_coarse_eqs_per_proc 1   #  <1>: Number of equations per process for coarse problem redistribution (significant only at the coarsest level) (none) 
@PARAMETER -pc_bddc_coarsening_ratio 8   #  <8>: Set coarsening ratio used in multilevel coarsening (none) 
@PARAMETER -pc_bddc_levels 0   #  <0>: Set maximum number of levels for multilevel (none) 
@PARAMETER -pc_bddc_use_coarse_estimates FALSE   # : <FALSE> Use estimated eigenvalues for coarse problem (none) 
@PARAMETER -pc_bddc_use_deluxe_scaling FALSE   # : <FALSE> Use deluxe scaling for BDDC (none) 
@PARAMETER -pc_bddc_schur_rebuild FALSE   # : <FALSE> Whether or not the interface graph for Schur principal minors has to be rebuilt (i.e. define the interface without any adjacency) (none) 
@PARAMETER -pc_bddc_schur_layers -1   #  <-1>: Number of dofs' layers for the computation of principal minors (i.e. -1 uses all dofs) (none) 
@PARAMETER -pc_bddc_schur_use_useradj FALSE   # : <FALSE> Whether or not the CSR graph specified by the user should be used for computing successive layers (default is to use adj of local mat) (none) 
@PARAMETER -pc_bddc_schur_exact FALSE   # : <FALSE> Whether or not to use the exact Schur complement instead of the reduced one (which excludes size 1 cc) (none) 
@PARAMETER -pc_bddc_deluxe_zerorows FALSE   # : <FALSE> Zero rows and columns of deluxe operators associated with primal dofs (none) 
@PARAMETER -pc_bddc_deluxe_singlemat FALSE   # : <FALSE> Collapse deluxe operators (none) 
@PARAMETER -pc_bddc_adaptive_userdefined FALSE   # : <FALSE> Use user-defined constraints (should be attached via MatSetNearNullSpace to pmat) in addition to those adaptively generated (none) 
@PARAMETER -pc_bddc_adaptive_threshold 0.   #  <0.>: Thresholds to be used for adaptive selection of constraints (none) 
@PARAMETER -pc_bddc_adaptive_nmin 0   #  <0>: Minimum number of constraints per connected components (none) 
@PARAMETER -pc_bddc_adaptive_nmax 0   #  <0>: Maximum number of constraints per connected components (none) 
@PARAMETER -pc_bddc_symmetric TRUE   # : <TRUE> Symmetric computation of primal basis functions (none) 
@PARAMETER -pc_bddc_coarse_adj 0   #  <0>: Number of processors where to map the coarse adjacency list (none) 
@PARAMETER -pc_bddc_benign_trick FALSE   # : <FALSE> Apply the benign subspace trick to saddle point problems with discontinuous pressures (none) 
@PARAMETER -pc_bddc_benign_change FALSE   # : <FALSE> Compute the pressure change of basis explicitly (none) 
@PARAMETER -pc_bddc_benign_compute_correction TRUE   # : <TRUE> Compute the benign correction during PreSolve (none) 
@PARAMETER -pc_bddc_nonetflux FALSE   # : <FALSE> Automatic computation of no-net-flux quadrature weights (none) 
@PARAMETER -pc_bddc_detect_disconnected FALSE   # : <FALSE> Detects disconnected subdomains (none) 
@PARAMETER -pc_bddc_eliminate_dirichlet FALSE   # : <FALSE> Whether or not we want to eliminate dirichlet dofs during presolve (none) 

