@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_syspfmg_print_statistics FALSE   # : <FALSE> Print statistics (HYPRE_SStructSysPFMGSetPrintLevel) 
@PARAMETER -pc_syspfmg_its 1   #  <1>: Number of iterations of SysPFMG to use as preconditioner (HYPRE_SStructSysPFMGSetMaxIter) 
@PARAMETER -pc_syspfmg_num_pre_relax 1   #  <1>: Number of smoothing steps before coarse grid (HYPRE_SStructSysPFMGSetNumPreRelax) 
@PARAMETER -pc_syspfmg_num_post_relax 1   #  <1>: Number of smoothing steps after coarse grid (HYPRE_SStructSysPFMGSetNumPostRelax) 
@PARAMETER -pc_syspfmg_tol 1e-08   #  <1e-08>: Tolerance of SysPFMG (HYPRE_SStructSysPFMGSetTol) 
@PARAMETER -pc_syspfmg_relax_type Red/Black-Gauss-Seidel   #  <Red/Black-Gauss-Seidel> Relax type for the up and down cycles (choose one of) Weighted-Jacobi Red/Black-Gauss-Seidel (HYPRE_SStructSysPFMGSetRelaxType) 

@PRECON telescope 
