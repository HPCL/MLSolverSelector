
##### Anything not starting with a @ is ignored
@DATABASE 
@PARAMETER Database ./new_test_file.sql

@MACHINELEARNING 
@PARAMETER algorithm RandomForest 
#@PARAMETER serialized model.serialized

@INTERFACE 
@PARAMETER build_inline false 
@PARAMETER dump_matrix false
@PARAMETER use_ml true

@MATRIX ./data/spd-real-int32-float32
@MATRIXDIR ./data/ .pbin  

@SOLVER cg 

@SOLVER gmres 
@PARAMETER ksp_gmres_restart 30 40 50 60 70
@PARAMETER ksp_gmres_haptol 1e-30
@PARAMETER ksp_gmres_preallocate FALSE
@PARAMETER ksp_gmres_classicalgramschmidt TRUE
@PARAMETER ksp_gmres_cgs_refinement_type REFINE_NEVER
@PARAMETER ksp_gmres_krylov_monitor FALSE

@SOLVER minres 

@PRECON hypre 
@PARAMETER pc_hypre_type boomeramg

@PRECON bjacobi

@PAIR cg bjacobi
@PAIR gmres bjacobi



