# List of options for each preconditioner 

@PRECON asm 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_asm_dm_subdomains FALSE   # : <FALSE> Use DMCreateDomainDecomposition() to define subdomains (PCASMSetDMSubdomains) 
@PARAMETER -pc_asm_blocks -1   #  <-1>: Number of subdomains (PCASMSetTotalSubdomains) 
@PARAMETER -pc_asm_overlap 1   #  <1>: Number of grid points overlap (PCASMSetOverlap) 
@PARAMETER -pc_asm_type RESTRICT   #  <RESTRICT> (choose one of) NONE RESTRICT INTERPOLATE BASIC (PCASMSetType) 
@PARAMETER -pc_asm_local_type ADDITIVE   #  <ADDITIVE> (choose one of) ADDITIVE MULTIPLICATIVE SYMMETRIC_MULTIPLICATIVE SPECIAL SCHUR (PCASMSetLocalType) 

@PRECON bddc 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_bddc_check_level 0   #  <0>: Verbose output for PCBDDC (intended for debug) (none) 
@PARAMETER -pc_bddc_use_local_mat_graph TRUE   # : <TRUE> Use or not adjacency graph of local mat for interface analysis (none) 
@PARAMETER -pc_bddc_use_vertices TRUE   # : <TRUE> Use or not corner dofs in coarse space (none) 
@PARAMETER -pc_bddc_use_edges TRUE   # : <TRUE> Use or not edge constraints in coarse space (none) 
@PARAMETER -pc_bddc_use_faces FALSE   # : <FALSE> Use or not face constraints in coarse space (none) 
@PARAMETER -pc_bddc_use_true_nnsp FALSE   # : <FALSE> Use near null space attached to the matrix without modifications (none) 
@PARAMETER -pc_bddc_use_qr_single FALSE   # : <FALSE> Use QR factorization for single constraints on cc (QR is used when multiple constraints are present) (none) 
@PARAMETER -pc_bddc_use_change_of_basis FALSE   # : <FALSE> Use or not internal change of basis on local edge nodes (none) 
@PARAMETER -pc_bddc_use_change_on_faces FALSE   # : <FALSE> Use or not internal change of basis on local face nodes (none) 
@PARAMETER -pc_bddc_switch_static FALSE   # : <FALSE> Switch on static condensation ops around the interface preconditioner (none) 
@PARAMETER -pc_bddc_coarse_redistribute 0   #  <0>: Number of procs where to redistribute coarse problem (none) 
@PARAMETER -pc_bddc_coarsening_ratio 8   #  <8>: Set coarsening ratio used in multilevel coarsening (none) 
@PARAMETER -pc_bddc_levels 0   #  <0>: Set maximum number of levels for multilevel (none) 
@PARAMETER -pc_bddc_use_coarse_estimates FALSE   # : <FALSE> Use estimated eigenvalues for coarse problem (none) 
@PARAMETER -pc_bddc_use_deluxe_scaling FALSE   # : <FALSE> Use deluxe scaling for BDDC (none) 
@PARAMETER -pc_bddc_schur_rebuild FALSE   # : <FALSE> Whether or not the interface graph for Schur principal minors has to be rebuilt (i.e. define the interface without any adjacency) (none) 
@PARAMETER -pc_bddc_schur_layers -1   #  <-1>: Number of dofs' layers for the computation of principal minors (i.e. -1 uses all dofs) (none) 
@PARAMETER -pc_bddc_schur_use_useradj FALSE   # : <FALSE> Whether or not the CSR graph specified by the user should be used for computing successive layers (default is to use adj of local mat) (none) 
@PARAMETER -pc_bddc_deluxe_faster FALSE   # : <FALSE> Faster application of deluxe scaling (requires extra work during setup) (none) 
@PARAMETER -pc_bddc_adaptive_threshold 0.   #  <0.>: Threshold to be used for adaptive selection of constraints (none) 
@PARAMETER -pc_bddc_adaptive_nmin 0   #  <0>: Minimum number of constraints per connected components (none) 
@PARAMETER -pc_bddc_adaptive_nmax 0   #  <0>: Maximum number of constraints per connected components (none) 
@PARAMETER -pc_bddc_symmetric TRUE   # : <TRUE> Symmetric computation of primal basis functions (none) 
@PARAMETER -pc_bddc_coarse_adj 0   #  <0>: Number of processors where to map the coarse adjacency list (none) 

@PRECON bjacobi 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_bjacobi_blocks -1   #  <-1>: Total number of blocks (PCBJacobiSetTotalBlocks) 

@PRECON cholesky 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_factor_in_place FALSE   # : <FALSE> Form factored matrix in the same memory as the matrix (PCFactorSetUseInPlace) 
@PARAMETER -pc_factor_fill 5.   #  <5.>: Expected non-zeros in factored matrix (PCFactorSetFill) 
@PARAMETER -pc_factor_shift_type NONE   #  <NONE> (choose one of) NONE NONZERO POSITIVE_DEFINITE INBLOCKS (PCFactorSetShiftType) 
@PARAMETER -pc_factor_shift_amount 0.   #  <0.>: Shift added to diagonal (PCFactorSetShiftAmount) 
@PARAMETER -pc_factor_zeropivot 2.22045e-14   #  <2.22045e-14>: Pivot is considered zero if less than (PCFactorSetZeroPivot) 
@PARAMETER -pc_factor_column_pivot 0.   #  <0.>: Column pivot tolerance (used only for some factorization) (PCFactorSetColumnPivot) 
@PARAMETER -pc_factor_pivot_in_blocks TRUE   # : <TRUE> Pivot inside matrix dense blocks for BAIJ and SBAIJ (PCFactorSetPivotInBlocks) 
@PARAMETER -pc_factor_reuse_fill FALSE   # : <FALSE> Use fill from previous factorization (PCFactorSetReuseFill) 
@PARAMETER -pc_factor_reuse_ordering FALSE   # : <FALSE> Reuse ordering from previous factorization (PCFactorSetReuseOrdering) 
@PARAMETER -pc_factor_mat_ordering_type natural   #  <natural>: Reordering to reduce nonzeros in factored matrix (one of) (PCFactorSetMatOrderingType) 
@PARAMETER -pc_factor_mat_solver_package (null)   #  <(null)>: Specific direct solver to use (MatGetFactor) 

@PRECON composite 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_composite_type ADDITIVE   #  <ADDITIVE> (choose one of) ADDITIVE MULTIPLICATIVE SYMMETRIC_MULTIPLICATIVE SPECIAL SCHUR (PCCompositeSetType) 
@PARAMETER -pc_composite_pcs string1,string2,...   #  <string1,string2,...>: List of composite solvers (PCCompositeAddPC) 

@PRECON cp 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 

@PRECON eisenstat 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_eisenstat_omega 1.   #  <1.>: Relaxation factor 0 < omega < 2 (PCEisenstatSetOmega) 
@PARAMETER -pc_eisenstat_no_diagonal_scaling FALSE   # : <FALSE> Do not use standard diagonal scaling (PCEisenstatSetNoDiagonalScaling) 

@PRECON exotic 
@PARAMETER -pc_use_amat TRUE   # : <TRUE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_exotic_type face   #  <face> (choose one of) face wirebasket (PCExoticSetType) 
@PARAMETER -pc_exotic_direct_solver FALSE   # : <FALSE> use direct solver to construct interpolation (None) 

@PRECON fieldsplit 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_fieldsplit_dm_splits TRUE   # : <TRUE> Whether to use DMCreateFieldDecomposition() for splits (PCFieldSplitSetDMSplits) 
@PARAMETER -pc_fieldsplit_block_size -1   #  <-1>: Blocksize that defines number of fields (PCFieldSplitSetBlockSize) 
@PARAMETER -pc_fieldsplit_diag_use_amat FALSE   # : <FALSE> Use Amat (not Pmat) to extract diagonal fieldsplit blocks (PCFieldSplitSetDiagUseAmat) 
@PARAMETER -pc_fieldsplit_off_diag_use_amat FALSE   # : <FALSE> Use Amat (not Pmat) to extract off-diagonal fieldsplit blocks (PCFieldSplitSetOffDiagUseAmat) 
@PARAMETER -pc_fieldsplit_type MULTIPLICATIVE   #  <MULTIPLICATIVE> (choose one of) ADDITIVE MULTIPLICATIVE SYMMETRIC_MULTIPLICATIVE SPECIAL SCHUR (PCFieldSplitSetType) 

@PRECON galerkin 
@PARAMETER -pc_use_amat FALSE   # : <FALSE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 

@PRECON gamg 
@PARAMETER -pc_use_amat TRUE   # : <TRUE> use Amat (instead of Pmat) to define preconditioner in nested inner solves (PCSetUseAmat) 
@PARAMETER -pc_gamg_type agg   #  <agg>: Type of AMG method (one of) geo agg classical (PCGAMGSetType) 
@PARAMETER -pc_gamg_repartition FALSE   # : <FALSE> Repartion coarse grids (PCGAMGRepartitioning) 
@PARAMETER -pc_gamg_reuse_interpolation FALSE   # : <FALSE> Reuse prolongation operator (PCGAMGReuseInterpolation) 
@PARAMETER -pc_gamg_use_agg_gasm FALSE   # : <FALSE> Use aggregation agragates for GASM smoother (PCGAMGUseASMAggs) 
@PARAMETER -pc_gamg_process_eq_limit 50   #  <50>: Limit (goal) on number of equations per process on coarse grids (PCGAMGSetProcEqLim) 
