@PARAMETER -pc_gamg_threshold 0.   #  <0.>: Relative threshold to use for dropping edges in aggregation graph (PCGAMGSetThreshold) 
@PARAMETER -pc_mg_levels 30   #  <30>: Set number of MG levels (PCGAMGSetNlevels) 
@PARAMETER -pc_gamg_agg_nsmooths 1   #  <1>: smoothing steps for smoothed aggregation, usually 1 (PCGAMGSetNSmooths) 
@PARAMETER -pc_gamg_sym_graph FALSE   # : <FALSE> Set for asymmetric matrices (PCGAMGSetSymGraph) 
@PARAMETER -pc_gamg_square_graph 1   #  <1>: Number of levels to square graph for faster coarsening and lower coarse grid complexity (PCGAMGSetSquareGraph) 
@PARAMETER -pc_gamg_random_type rander48   #  <rander48>: PetscRandom type (one of) rand rand48 rander48 (PetscRandomSetType) 
@PARAMETER -pc_gamg_random_seed 0   #  <0>: Seed to use to generate random numbers (PetscRandomSetSeed) 
